
      //Method convert from linkedList to array in DoubleLinkedList
 public String[] ConvertLinkedListToArray(){
           String[] a;
    if(this.count()==0)
        return a=null;
    else{
    
    a=new String[this.count()];
    DoubleNode cur=head;
    for(int i=0;i<a.length;i++){
    a[i]=cur.data;
    cur=cur.next;
    }
    
       return a;
    }
        
    
    } 
     
     
