    public static void main(String[] args) {
Graph g=new Graph(5);
g.addEdge("v1", "v5");
g.addEdge("v6","v6");

g.addEdge("v1", "v4");
g.addEdge("v2", "v3");
g.addEdge("v1", "v2");
g.addEdge("v2", "v3");

g.numberofEdges();
//g.BFS();
